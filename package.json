{
  "name": "sweatshop-calabasas",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/src/app.js",
    "prodstart": "node ./dist/app.js",
    "dockerstart": "docker run --rm -it --env-file ./.env -p 2222:2222/tcp -p 3001:3001/tcp -p 8080:8080/tcp sweatshopcalabasas:latest",
    "devstart": "tsc-watch --noClear -p ./tsconfig.json --onSuccess \"node ./dist/src/app.js\"",
    "build": "tsc",
    "typeorm": "typeorm-ts-node-commonjs",
    "migrate": "rm -rf dist && npm run build && typeorm-ts-node-commonjs migration:generate ./src/migrations/addedEntity -d ./src/database/dataSource.ts",
    "db:push": "rm -rf dist && npm run build && typeorm-ts-node-commonjs migration:run -d ./src/database/dataSource.ts"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "config": "^3.3.8",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "~4.16.1",
    "helmet": "^6.0.0",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.7.0",
    "passport": "^0.6.0",
    "passport-local-mongoose": "^7.1.2",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.10",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/config": "^3.3.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/http-errors": "^2.0.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.7",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "morgan": "^1.10.0",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3"
  }
}
